<% form_for @company do |f| %>
  <%= f.error_messages %>
  <p><%= f.label :nit, "NIT de la empresa" %><br />
    <%= f.text_field :nit %></p>

  <p><%= f.label :name, "Nombre" %><br />
    <%= f.text_field :name %></p>

  <p> <%= f.label :email, "Correo electronico" %><br />
    <%= f.text_field :email %></p>

  <p><%= f.label :telephone, "Telefono" %><br />
    <%= f.text_field :telephone %></p>
  <p><%= f.submit "Registrar" %></p>
<% end %>
