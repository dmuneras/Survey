<% form_for @company do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :nit %><br />
    <%= f.text_field :nit %>
  </p>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.label :telephone %><br />
    <%= f.text_field :telephone %>
  </p>
 <p><%= f.submit %></p>
<% end %>
