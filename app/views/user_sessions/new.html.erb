<div class= "contentWrap" id= "login">
<div class="all-login">
  <div class= "cancel"><%= link_to (image_tag "close.png"), root_url %></div>
  <div class ="login-content">
    <h1>Ingreso Usuario</h1>
    <% form_for @user_session do |f| %>
    <%= f.error_messages %>
    <p>
      <%= f.label :username, "Ingrese nombre de usuario" %><br />
      <%= f.text_field :username %>
    </p>
    <p>
      <%= f.label :password, "Ingrese contraseÃ±a" %><br />
      <%= f.password_field :password %>
    </p>
    <p>
       <%= f.submit "Ingresar" %> 
    </p>
    <% end %>
    </div>
  </div>
</div>
</div>


